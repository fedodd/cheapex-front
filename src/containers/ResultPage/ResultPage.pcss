.resultPageWrapper {
	flex-direction: column;
	padding-right: 15px;
	padding-left: 15px;
	box-sizing: border-box;
	height: 100vh;
	background-color: white;
	display: flex;
}

.resultPage {
	overflow: scroll;
	position: relative;
	flex: 1;

	/* & > * {
		position: sticky;
		left: 0;
	} */
	/* hide scrollbar */
	&::-webkit-scrollbar {
		display: none;
	}
}

.noDataList {
	padding: 0;
	list-style: none;

	& li {
		margin: 0;
		padding: 0;
	}
}
